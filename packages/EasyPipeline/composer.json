{
    "name": "stepthefkup/easy-pipeline",
    "description": "Provides an easy and powerful way to implement pipelines for anything",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2",
        "illuminate/pipeline": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "StepTheFkUp\\EasyPipeline\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StepTheFkUp\\EasyPipeline\\Tests\\": "tests"
        }
    },
    "suggest": {
        "illuminate/pipeline": "To use the Illuminate Pipeline implementation"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
