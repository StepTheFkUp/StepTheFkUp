{
    "name": "eonx-com/easy-webhook",
    "description": "Sending webhooks has never been so easy (persistence, retry, async)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "eonx-com/easy-event-dispatcher": "^3.0",
        "eonx-com/easy-lock": "^3.0",
        "eonx-com/easy-random": "^3.0",
        "eonx-com/easy-utils": "^3.0",
        "nesbot/carbon": "^2.22",
        "nette/utils": "^3.1",
        "symfony/http-client": "^4.4 || ^5.1.5"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6",
        "eonx-com/easy-pagination": "^3.0",
        "illuminate/database": "^5.5 || ^6.0",
        "ramsey/uuid": "^3.9",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyWebhook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyWebhook\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "suggest": {
        "eonx-com/easy-pagination": "Used to pagined due webhooks for sendAfter feature"
    }
}
